# Frontend Environment Variables
# Copy this file to .env.local for local development
# These are set in Amplify Hosting environment variables for production

# API Configuration
NUXT_PUBLIC_API_URL=https://your-api-gateway-url.execute-api.eu-west-1.amazonaws.com
NUXT_PUBLIC_API_BASE_PATH=/Prod

# App URL (for QR codes and absolute URLs)
NUXT_PUBLIC_APP_URL=https://your-app-domain.com

# Cognito Configuration
NUXT_PUBLIC_COGNITO_USER_POOL_ID=eu-west-1_xxxxxxxxx
NUXT_PUBLIC_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
NUXT_PUBLIC_COGNITO_REGION=eu-west-1

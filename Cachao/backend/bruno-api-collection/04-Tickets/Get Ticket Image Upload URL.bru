meta {
  name: Get Ticket Image Upload URL
  type: http
  seq: 5
}

post {
  url: {{api_url}}/events/{{event_id}}/tickets/{{ticket_id}}/image-upload-url
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{id_token}}
}

body:json {
  {
    "content_type": "image/jpeg",
    "file_name": "ticket-image.jpg"
  }
}
